{
  "name": "generator",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "build": "babel src --out-dir lib --copy-files",
    "start": "nodemon --exec babel-node src/app.js",
    "serve": "npm run build && node lib/app.js",
    "lint": "eslint src",
    "test:jest": "jest",
    "test": "npm run lint && npm run test:jest",
    "test:watch": "jest '--watch'",
    "test-coverage": "jest --coverage",
    "nsp:check": "nsp check --reporter summary",
    "husky": {
      "hooks": {
        "pre-push": "npm test"
      }
    },
    "audit": "npx check-audit --registry https://registry.npmjs.org/",
    "audit-ignore": "npx resolve-audit --registry https://registry.npmjs.org/",
    "audit-fix": "npm audit fix --registry https://registry.npmjs.org/"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime": "^7.5.5",
    "axios": "^0.19.0",
    "body-parser": "~1.19.0",
    "cors": "~2.8.5",
    "ejs": "~2.7.1",
    "express": "~4.17.1",
    "googleapis": "~42.0.0",
    "lodash": "^4.17.15",
    "moment": "~2.24.0",
    "mongoose": "^5.6.12",
    "nodemailer": "~6.3.0",
    "promise": "~8.0.3",
    "npm-audit-resolver": "^2.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.3",
    "eslint": "~6.3.0",
    "eslint-config-airbnb-base": "~14.0.0",
    "eslint-import-resolver-webpack": "~0.11.1",
    "eslint-loader": "~3.0.0",
    "eslint-plugin-babel": "~5.3.0",
    "eslint-plugin-import": "~2.18.2",
    "glob": "~7.1.4",
    "husky": "~3.0.8",
    "jest": "^25.0.0",
    "nodemon": "^1.19.2",
    "webpack": "~4.39.3",
    "webpack-node-externals": "~1.7.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "./coverage"
  }
}
